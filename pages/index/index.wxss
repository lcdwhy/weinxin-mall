swiper {
  height: 340rpx;
  width: 750rpx;
}
swiper image {
  width: 100%;
}
.category-box {
  display: flex;
}
.category-box navigator {
  flex: 1;
  padding: 20rpx;
}
.category-box navigator image {
  width: 100%;
}
.index_floot .floot_group .floot_title image {
  width: 100%;
}
.index_floot .floot_group .floot_list {
  padding: 10rpx 0;
  overflow: hidden;
}
.index_floot .floot_group .floot_list navigator {
  float: left;
  width: 33.33%;
}
.index_floot .floot_group .floot_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid white;
}
.index_floot .floot_group .floot_list navigator:nth-child(2),
.index_floot .floot_group .floot_list navigator:nth-child(3) {
  border-bottom: 10rpx solid white;
}
.index_floot .floot_group .floot_list navigator image {
  width: 100%;
  height: 100%;
}
